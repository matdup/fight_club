<div class="container">
  <div class="row">
    <div class="col-4 offset-4">
      <%= simple_form_for @arena do |f| %>
        <%= f.input :title %>

        <%= f.input :description %>

        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>

        <%= f.input :price %>

        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>




